{
  "name": "sex-web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite --open",
    "start:prod": "vite --open --mode production",
    "build": "vite build --mode test",
    "build:test": "tsc && vite build --mode test",
    "preview": "vite preview",
    "lint": "yarn eslint && yarn prettier && yarn stylelint",
    "eslint": "eslint --cache '@(src)/**/*.@(js|jsx|ts|tsx)'",
    "eslint:fix": "yarn eslint -- --fix",
    "prettier": "prettier --write '@(src)/**/*.@(ts|tsx)'",
    "stylelint": "stylelint '@(src)/**/*.@(css|scss|less)'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "charting_library": "file:public/charting_library",
    "ahooks": "^3.7.5",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.2",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.7",
    "ethers": "^5",
    "framer-motion": "^10.2.3",
    "lodash-es": "^4.17.21",
    "mac-scrollbar": "^0.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-p5": "^1.3.33",
    "react-remove-scroll": "^2.5.5",
    "react-router-dom": "^6.9.0",
    "recoil": "^0.7.7",
    "styled-components": "^6.0.0-beta.13",
    "wagmi": "^0.12.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@iceworks/spec": "^1.6.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/recoil": "^0.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vite-plugin-require": "^1.1.10",
    "vite-plugin-virtual-mpa": "^1.7.0"
  }
}
